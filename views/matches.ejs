<% include ./partials/head.ejs %>
<title>Gamerdate Home</title>
</head>

<body>
	<header>
		<% include ./partials/header.ejs %>
		<h1> Gamerdate <span>Your matches</span></h1>
	</header>
	<main>
		<% for (var i = 0; i < matches.length; i++) { %>
		<section>
			<header>
				<%= matches[i].username %>
			</header>
			<p>
				<%= matches[i].games %>
			</p>
		</section>
		<% } %>
	</main>
